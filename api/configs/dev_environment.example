#!/usr/bin/env bash

# Schema:
# ENVIRONMENT_KEY=Value
#
# Please copy `./api/configs/dev_environment.example` to `./api/configs/dev_environment`
# and fill in the missing information!

LOGLEVEL=DEBUG
FLASK_DEBUG=True

POSTGRES_DB=cut_dev
POSTGRES_HOST=postgis
POSTGRES_USER=cut_dev
POSTGRES_PASSWORD=postgres
POSTGRES_PORT=5432

GEOSERVER_WORKSPACE=CUT
GEOSERVER_ADMIN_USER=admin
GEOSERVER_ADMIN_PASSWORD=geoserver
GEOSERVER_BASE_URL=http://geoserver:8080/geoserver

# The SERVER_URL is only used to return the complete URL in the result of the job details as specified in OGC.
# Should be the base url to the api.
SERVER_URL=localhost:3000
